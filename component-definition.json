    {
      "title": "Form Components",
      "id": "form-general",
      "components": [
          {
            "title": "Text Input",
            "id": "text",
            "plugins": {
                "fnk": {
                    "form": {
                        "Type": "text",
                        "Label": "Text Input"
                    }
                }
            }
          },
          {
            "title": "Text Area",
            "id": "textarea",
            "plugins": {
                "fnk": {
                    "form": {
                        "Type": "textarea",
                        "Label": "Text Area"
                    }
                }
            }
          },
          {
              "title": "Number Input",
              "id": "numberinput",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "number",
                          "Label": "Number Input"
                      }
                  }
              }
          },
          {
              "title": "Date Input",
              "id": "dateinput",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "date",
                          "Label": "Date Input"
                      }
                  }
              }
          },
          {
              "title": "Drop Down",
              "id": "dropdown",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "select",
                          "Label": "Drop Down List",
                          "Options": ["Option 1", "Option 2"]
                      }
                  }
              }
          },
          {
              "title": "Check Box",
              "id": "checkboxinput",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "checkbox",
                          "Label": "Check Box",
                          "Value": "Checkbox Item"
                      }
                  }
              }
          },
          {
              "title": "Radio Button",
              "id": "radiobutton",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "radio",
                          "Label": "Radio Button",
                          "Value": "Radio Item"
                      }
                  }
              }
          },
          {
              "title": "Button",
              "id": "button",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "button",
                          "Label": "Button"
                      }
                  }
              }
          },
          {
              "title": "Submit Button",
              "id": "submit",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "submit",
                          "Label": "Submit"
                      }
                  }
              }
          },
          {
              "title": "File Attachment",
              "id": "fileinput",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "file",
                          "Label": "File Attachment",
                          "Accept": ["audio/*", "video/*", "image/*", "text/*", "application/pdf"]
                      }
                  }
              }
          },
          {
              "title": "Panel",
              "id": "panel",
              "plugins": {
                  "fnk": {
                      "form": {
                          "Type": "fieldset",
                          "Label": "Panel"
                      }
                  }
              }
          }
